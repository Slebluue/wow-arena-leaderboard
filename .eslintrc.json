{
  "extends": ["next/core-web-vitals", "next", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "object-curly-spacing": [2, "always"]
  }
}
